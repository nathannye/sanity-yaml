{{#if (shouldRemoveDefineField)}}
{
    name: '{{name}}',
    type: 'object',
    fields: [
        {{> sanity-fields fields=fields}}
    ]{{> sanity-validation validation=validation}}
},
{{else}}
defineField({
    name: '{{name}}',
    type: 'object',
    fields: [
        {{> sanity-fields fields=fields}}
    ]{{> sanity-validation validation=validation}}
}),
{{/if}}
